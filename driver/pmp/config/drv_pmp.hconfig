    config USE_DRV_PMP
        bool "Use PMP Driver?"
        select DRIVER
        default n if !USE_DRV_PMP_NEEDED
        default y if USE_DRV_PMP_NEEDED
        ---help---
        IDH_HTML_PMP_Peripheral_Library
        ---endhelp---

    config DRV_PMP_IMPL
        string "Driver Implementation"
        depends on USE_DRV_PMP
        range DRV_PMP_IMPLEMENTATION
